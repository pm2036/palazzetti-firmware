
config uhttpd 'main'
	list listen_http '0.0.0.0:80'
	list listen_http '[::]:80'
#	list listen_https '0.0.0.0:443'
#	list listen_https '[::]:443'
#   option redirect_https '1'
	option home '/www'
	option rfc1918_filter '1'
	option max_requests '3'
	option max_connections '100'
	option cert '/etc/uhttpd.crt'
	option key '/etc/uhttpd.key'
	option cgi_prefix '/www/cgi-bin'
	option lua_prefix '/cgi-bin'
	option lua_handler '/www/cgi-bin/index.lua'
	option error_page '/cgi-bin/error.lua'
	option script_timeout '60'
	option network_timeout '60'
	option http_keepalive '60'
	option tcp_keepalive '0'
	option ubus_prefix '/ubus'
	list interpreter '.lua=/usr/bin/lua'


config cert 'px5g'
	option days '730'
	option bits '1024'
	option country 'IT'
	option state 'Pordenone'
	option location 'Porcia'
	option commonname 'connectionbox'

